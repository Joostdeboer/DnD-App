{
  "name": "maroon-stingray",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "run": "sanity build && sanity start",
    "cors:add": "sanity cors add",
    "deploy-graphql": "sanity graphql deploy",
    "typegen": "sanity schema extract --path=./../frontend/src/sanity/extract.json && sanity typegen generate",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/vision": "^4.0.0",
    "@types/react-dom": "19.1.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-is": "^18.3.1",
    "sanity": "^4.0.0",
    "sanity-plugin-media": "^3.0.4",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^2.0.4",
    "@types/react": "19.1.6",
    "@types/styled-components": "^5.1.34",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "typescript": "^5.8.3"
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  }
}
